    <validationRules>
        <fullName>Only_the_hours_field_matching_the_select</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
    ISCHANGED(Date__c) || 
    ISCHANGED(Type__c) || 
    ISCHANGED(Project__c) || 
    ISCHANGED(Project_Budget__c) || 
    ISCHANGED(Project_Resource__c) || 
    ISCHANGED(Project_Task__c) || 
    ISCHANGED(Project_Role__c) || 
    ISCHANGED(Resource__c) || 
    ISCHANGED(Role__c)
)</errorConditionFormula>
        <errorMessage>Only the hours field matching the selected type can be changed. All other fields are not editable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_the_relevant_field_may_be_populated</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
    /* If Type is Budget, ensure only Budget_hrs__c is populated */
    (ISPICKVAL(Type__c, &quot;Budget&quot;) &amp;&amp; 
        (NOT(ISNULL(Available_hrs__c)) || NOT(ISNULL(Billable_hrs__c)) || NOT(ISNULL(Non_Billable_hrs__c)) || NOT(ISNULL(Planned_hrs__c)))
    ) ||
    /* If Type is Available, ensure only Available_hrs__c is populated */
    (ISPICKVAL(Type__c, &quot;Available&quot;) &amp;&amp; 
        (NOT(ISNULL(Budget_hrs__c)) || NOT(ISNULL(Billable_hrs__c)) || NOT(ISNULL(Non_Billable_hrs__c)) || NOT(ISNULL(Planned_hrs__c)))
    ) ||
    /* If Type is Planned, ensure only Planned__c is populated */
    (ISPICKVAL(Type__c, &quot;Planned&quot;) &amp;&amp; 
        (NOT(ISNULL(Available_hrs__c)) || NOT(ISNULL(Billable_hrs__c)) || NOT(ISNULL(Non_Billable_hrs__c)) || NOT(ISNULL(Budget_hrs__c)))
    ) ||
    /* If Type is Billable, ensure only Billable_hrs__c is populated */
    (ISPICKVAL(Type__c, &quot;Billable&quot;) &amp;&amp; 
        (NOT(ISNULL(Available_hrs__c)) || NOT(ISNULL(Budget_hrs__c)) || NOT(ISNULL(Non_Billable_hrs__c)) || NOT(ISNULL(Planned_hrs__c)))
    ) ||
    /* If Type is Non-Billable, ensure only Non_Billable__c is populated */
    (ISPICKVAL(Type__c, &quot;Non-Billable&quot;) &amp;&amp; 
        (NOT(ISNULL(Available_hrs__c)) || NOT(ISNULL(Budget_hrs__c)) || NOT(ISNULL(Billable_hrs__c)) || NOT(ISNULL(Planned_hrs__c)))
    )
)</errorConditionFormula>
        <errorMessage>Only the relevant field may be populated based on the selected Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>You_can_only_update_the_hours_field_that</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
    ISCHANGED(Target_hrs__c) &amp;&amp; NOT(ISPICKVAL(Type__c, &quot;Target&quot;)) ||
    ISCHANGED(Available_hrs__c) &amp;&amp; NOT(ISPICKVAL(Type__c, &quot;Available&quot;)) ||
    ISCHANGED(Billable_hrs__c) &amp;&amp; NOT(ISPICKVAL(Type__c, &quot;Billable&quot;)) ||
    ISCHANGED(Budget_hrs__c) &amp;&amp; NOT(ISPICKVAL(Type__c, &quot;Budget&quot;)) ||
    ISCHANGED(Non_Billable_hrs__c) &amp;&amp; NOT(ISPICKVAL(Type__c, &quot;Non-Billable&quot;)) ||
    ISCHANGED(Planned_hrs__c) &amp;&amp; NOT(ISPICKVAL(Type__c, &quot;Planned&quot;))
)</errorConditionFormula>
        <errorMessage>You can only update the hours field that matches the selected type.</errorMessage>
    </validationRules>
</CustomObject>