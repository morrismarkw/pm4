  OR(
    WEEKDAY(Date__c) = 1,  
    WEEKDAY(Date__c) = 7   
  ),
  IMAGE(&quot;/resource/pm_images/path_light_gray.png&quot;, &quot;Weekend&quot;, 16, 64),  
  IMAGE(&quot;/resource/pm_images/path_dark_purple.png&quot;, &quot;Weekday&quot;, 16, 64)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
