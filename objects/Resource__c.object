    <fields>
        <fullName>Planning_Start_Date__c</fullName>
        <defaultValue>TODAY()</defaultValue>
        <inlineHelpText>The date that planning was begun for this Resource.</inlineHelpText>
        <label>Planning Start Date</label>
        <required>true</required>
