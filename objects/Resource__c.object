    <validationRules>
        <fullName>User_is_required</fullName>
        <active>true</active>
        <description>The User lookup can not be made required, thus a validation rule is needed.</description>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
     ISBLANK( User__c )
)</errorConditionFormula>
        <errorDisplayField>User__c</errorDisplayField>
        <errorMessage>User is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>User_can_not_be_changed</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
     !ISNEW() &amp;&amp;  ISCHANGED( User__c ) 
)
/* User is required when the record is created, but it can not be updated */</errorConditionFormula>
        <errorMessage>User can not be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Default_Target_Utilization_FTE_must_b</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
      Default_Target_FTE_weekday__c &lt; 0 ||  Default_Target_FTE_weekday__c &gt; 100
)</errorConditionFormula>
        <errorMessage>Default Target Utilization (%FTE) must be from 0% to 100%.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Default_Target_Utilization_Total_must</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
      Default_Target_Available_weekday__c  &lt; 0 ||  Default_Target_Available_weekday__c &gt; 100
)</errorConditionFormula>
        <errorMessage>Default Target Utilization (%Total) must be from 0% to 100%.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Default_Total_Time_FTE_must_be_from_0</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
      Default_Available_FTE_weekday__c  &lt; 0 ||  Default_Available_FTE_weekday__c &gt; 100
)</errorConditionFormula>
        <errorMessage>Default Total Time (%FTE) must be from 0% to 100%.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Planning_Start_Date_can_not_be_changed</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
     ISCHANGED(Planning_Start_Date__c) &amp;&amp;
     !ISNEW()
)</errorConditionFormula>
        <errorMessage>Planning Start Date can not be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Planning_Start_Date_must_be_greater_than</fullName>
        <active>true</active>
        <description>Planning Start Date must be greater than or equal to today.</description>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
     Planning_Start_Date__c &lt; TODAY() &amp;&amp;
     ISNEW()
)</errorConditionFormula>
        <errorMessage>Planning Start Date must be greater than or equal to today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Resource_Planning_Horizon_must_be_from_3</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
     Planning_Horizon_days__c &gt; 90 || Planning_Horizon_days__c &lt; 30
)</errorConditionFormula>
        <errorMessage>Resource Planning Horizon must be from 30 to 90 days.</errorMessage>
    </validationRules>
</CustomObject>