    <validationRules>
        <fullName>Stage_must_always_move_forward</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
     ISCHANGED(Stage__c) &amp;&amp; 
     Stage_Number__c &lt; PRIORVALUE( Stage_Number__c )
)</errorConditionFormula>
        <errorMessage>Stage must always move forward.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_must_be_after_the_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
     ISCHANGED(End_Date__c) &amp;&amp;
     End_Date__c &lt;= Start_Date__c
)</errorConditionFormula>
        <errorMessage>End Date must be after the Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_can_not_be_set_to_the_past</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
     ISCHANGED(Start_Date__c) &amp;&amp;
     Start_Date__c &lt; TODAY()
)</errorConditionFormula>
        <errorMessage>Start Date can not be set to the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_must_be_before_the_End_Date</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
     ISCHANGED(Start_Date__c) &amp;&amp;
     Start_Date__c &gt;= End_Date__c
)</errorConditionFormula>
        <errorMessage>Start Date must be before the End Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Budget_must_be_allocated_to_the_Project</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
     ISCHANGED(Stage__c) &amp;&amp; 
     ISPICKVAL(Stage__c,&quot;In Progress&quot;) &amp;&amp;
     Budget__c &lt;= 0
)</errorConditionFormula>
        <errorMessage>Budget must be allocated to the Project before it can be moved to In Progress.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_can_not_be_changed_once_the_Sta</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
     ISCHANGED(End_Date__c) &amp;&amp; 
     PRIORVALUE(Stage_Number__c)&gt; 2
)</errorConditionFormula>
        <errorMessage>End Date can not be changed once the Stage is moved to Completed or Canceled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_must_be_less_than_Start_Date_pl</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
    End_Date__c &gt; DATE(
        YEAR(Start_Date__c) + 1,
        MONTH(Start_Date__c),
        DAY(Start_Date__c)
    )
)</errorConditionFormula>
        <errorMessage>End Date must be less than Start Date plus one year.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_must_be_set_to_today_before_mov</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
     ISCHANGED(Stage__c) &amp;&amp; (
          ISPICKVAL(Stage__c,&quot;Completed&quot;) || 
          ISPICKVAL(Stage__c,&quot;Canceled&quot;)
     ) &amp;&amp; 
     End_Date__c != TODAY()
)</errorConditionFormula>
        <errorMessage>End Date must be set to today before moving the Stage to Completed or Canceled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_can_not_be_changed_once_the_S</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
     ISCHANGED(Start_Date__c) &amp;&amp; 
      PRIORVALUE(Stage_Number__c) &gt; 1
)</errorConditionFormula>
        <errorMessage>Start Date can not be changed once the Stage is moved to In Progress.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_must_be_set_to_today_before_m</fullName>
        <active>true</active>
        <errorConditionFormula>!Bypass_Validation__c &amp;&amp; (
     ISCHANGED(Stage__c) &amp;&amp; 
     ISPICKVAL(Stage__c,&quot;In Progress&quot;) &amp;&amp; 
     Start_Date__c != TODAY() 
)</errorConditionFormula>
        <errorMessage>Start Date must be set to today before moving the Stage to In Progress.</errorMessage>
    </validationRules>
