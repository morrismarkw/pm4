    <fields>
        <fullName>Planned_Tasks_hrs_weekday__c</fullName>
        <externalId>false</externalId>
        <formula>Planned_Tasks_hrs__c / Duration_weekdays__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Planned Tasks (hrs/weekday)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
